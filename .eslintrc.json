{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "eslint-plugin-html"
  ],
  "parser": "babel-eslint",
  "rules": {
    "require-jsdoc": "off",
    "no-unused-vars": ["off", 0],
    "indent": ["error", 2],
    "max-len": "off"
  },
  "settings": {
    "react": {
      "version": "15.0"
    }
  }

}
